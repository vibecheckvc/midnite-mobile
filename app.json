{
  "expo": {
    "name": "midnite",
    "slug": "midnite",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.midnite.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIAppFonts": []
      },
      "buildNumber": "13",
      "icon": "./assets/icon.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.midnite"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "43e84670-e0de-4b90-bd0f-8be28e4cdde1"
      }
    },
    "plugins": ["expo-font"]
  }
}
